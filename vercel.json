{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    },
    "vercel-api-test.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/vercel-api-test",
      "dest": "/vercel-api-test.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ]
} 